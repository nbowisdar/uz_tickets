# Example of .env file for .env.docker
# Bot
BOT_TOKEN=<BOT_TOKEN_FROM_BOT_FATHER>
ADMIN_IDS=[85644856, 5468546845]
DEBUG=False
USE_WEBHOOK=False
BOT_SECRET_TOKEN=<BOT_SECRET_TOKEN>


# API
API_NAME=bot
API_V1_STR=/api/v1
